{
  "mcpServers": {
    "filesystem": {
      "command": "$HOME/.local/share/mise/installs/node/VERSION_NODE/bin/npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "$HOME/Codes"],
      "env": {
        "PATH": "$HOME/.local/share/mise/installs/node/VERSION_NODE/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    "fetch": {
      "command": "$HOME/.local/share/mise/installs/python/VERSION_PYTHON/bin/uvx",
      "args": ["mcp-server-fetch"],
      "env": {
        "PATH": "$HOME/.local/share/mise/installs/python/VERSION_PYTHON/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    "puppeteer": {
      "command": "$HOME/.local/share/mise/installs/node/VERSION_NODE/bin/npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"],
      "env": {
        "PATH": "$HOME/.local/share/mise/installs/node/VERSION_NODE/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    "github": {
      "command": "$HOME/.local/share/mise/installs/node/VERSION_NODE/bin/npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$GITHUB_TOKEN",
        "PATH": "$HOME/.local/share/mise/installs/node/VERSION_NODE/bin:/usr/local/bin:/usr/bin:/bin"
      }
    }
  }
}
